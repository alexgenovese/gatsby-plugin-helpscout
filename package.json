{
  "name": "gatsby-plugin-helpscout",
  "version": "1.0.0",
  "description": "Gatsby plugin to add Help Scout to your site.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "help-scount"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexgenovese/gatsby-plugin-helpscout.git"
  },
  "author": "Alex Genovese (https://alexgenovese.it/)",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "cross-env": "7.0.3",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1"
  },
  "dependencies": {
    "babel-runtime": "6.26.0"
  }
}
